{
  "name": "coding-agent",
  "version": "0.1.0",
  "description": "AI Programming Assistant - A CLI tool for natural language programming conversations",
  "type": "module",
  "main": "dist/cli/index.js",
  "bin": {
    "coding-agent": "dist/cli/index.js"
  },
  "exports": {
    "./package.json": "./package.json",
    ".": {
      "import": "./dist/cli/index.js",
      "types": "./dist/cli/index.d.ts"
    }
  },
  "scripts": {
    "build": "vite build",
    "build:tsc": "tsc",
    "dev": "vite build --watch",
    "start": "node dist/cli/index.js",
    "start:web": "node dist/web/start.js",
    "dev:web": "vite build --watch & npm run start:web",
    "clean": "rm -rf dist",
    "prepublishOnly": "npm run clean && npm run build:all",
    "test": "jest --passWithNoTests --no-watch",
    "dev:frontend": "cd frontend && npm run dev",
    "build:frontend": "cd frontend && npm run build",
    "preview:frontend": "cd frontend && npm run preview",
    "dev:fullstack": "concurrently \"npm run dev:web\" \"npm run dev:frontend\"",
    "build:all": "vite build && npm run build:frontend",
    "start:fullstack": "concurrently \"npm run start:web\" \"npm run preview:frontend\"",
    "postbuild": "chmod +x dist/cli/index.js",
    "typecheck": "tsc --noEmit"
  },
  "keywords": [
    "ai",
    "programming",
    "assistant",
    "cli",
    "coding",
    "agent"
  ],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "@anthropic-ai/sdk": "^0.52.0",
    "@google/generative-ai": "^0.24.1",
    "@monaco-editor/react": "^4.5.0",
    "@types/prismjs": "1.26.5",
    "@types/react": "^19.1.6",
    "@types/uuid": "10.0.0",
    "axios": "^1.9.0",
    "body-parser": "^2.2.0",
    "chalk": "^5.3.0",
    "clsx": "^2.1.0",
    "commander": "^12.0.0",
    "cors": "^2.8.5",
    "express": "^5.1.0",
    "express-rate-limit": "^7.5.0",
    "fs-extra": "^11.2.0",
    "fuzzy": "^0.1.3",
    "globby": "^13.2.2",
    "helmet": "^8.1.0",
    "ink": "^4.4.1",
    "inquirer": "^9.2.0",
    "inquirer-autocomplete-prompt": "^3.0.1",
    "minimatch": "^10.0.1",
    "openai": "^4.28.0",
    "prism-react-renderer": "^2.1.0",
    "prismjs": "1.30.0",
    "react": "^18.3.1",
    "react-dom": "^18.2.0",
    "react-error-boundary": "^4.0.11",
    "react-helmet-async": "^1.3.0",
    "react-hot-toast": "^2.4.1",
    "react-markdown": "^8.0.7",
    "rehype-raw": "^7.0.0",
    "remark-gfm": "^3.0.1",
    "socket.io": "^4.8.1",
    "socket.io-client": "^4.7.5",
    "tree-sitter-javascript": "^0.23.1",
    "tree-sitter-python": "^0.23.6",
    "tree-sitter-rust": "^0.24.0",
    "tree-sitter-typescript": "^0.23.2",
    "uuid": "11.1.0",
    "web-tree-sitter": "^0.25.5",
    "zod": "^3.25.46"
  },
  "devDependencies": {
    "@types/body-parser": "^1.19.5",
    "@types/cors": "^2.8.18",
    "@types/express": "^5.0.2",
    "@types/fs-extra": "^11.0.0",
    "@types/inquirer": "^9.0.0",
    "@types/inquirer-autocomplete-prompt": "^3.0.3",
    "@types/jest": "^29.5.14",
    "@types/minimatch": "^5.1.2",
    "@types/node": "^20.0.0",
    "jest": "^29.7.0",
    "ts-jest": "^29.3.4",
    "tsx": "^4.19.4",
    "typescript": "^5.0.0",
    "typescript-transform-paths": "^3.5.5",
    "vite": "6.3.5"
  },
  "engines": {
    "node": ">=18.0.0"
  },
  "files": [
    "dist/**/*"
  ]
}
